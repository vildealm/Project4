{
   "name": "backend",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/node": "^14.11.8",
      "gql2ts": "^1.10.1",
      "graphql-request": "^3.1.0",
      "jest": "^26.5.2",
      "nodemon": "^2.0.4",
      "ts-jest": "^26.4.1",
      "ts-node": "9.0.0",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "4.0.3"
   },
   "dependencies": {
      "@apollo/client": "^3.2.5",
      "@types/jest": "^26.0.14",
      "graphql": "^15.3.0",
      "graphql-yoga": "^1.18.3",
      "pg": "^8.4.1",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.28"
   },
   "scripts": {
      "start": "nodemon --exec ts-node --files src/index.ts",
      "test": "jest",
      "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
